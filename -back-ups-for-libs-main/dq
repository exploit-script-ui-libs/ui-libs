                ColorSequenceKeypoint.new(1, Color3.fromRGB(36, 36, 36))
            }
        }
        c.InputBegan:Connect(
            function(c)
                for d, e in next, b.flags do
                    if (e == c.KeyCode) then
                        b.callbacks[d]()
                    elseif (e == c.UserInputType) then
                        b.callbacks[d]()
                    end
                end
            end
        )
        local f = Instance.new("ScreenGui", f)
        f.Enabled = false
        b.gui = f
        c.InputBegan:Connect(
            function(b, c)
                if c then
                    return
                end
                if b.KeyCode == Enum.KeyCode.RightControl then
                    f.Enabled = not f.Enabled
                end
            end
        )
