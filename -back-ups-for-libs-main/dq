                    [Enum.KeyCode.LeftControl] = "L-Ctrl",
                    [Enum.KeyCode.RightControl] = "R-Ctrl",
                    [Enum.KeyCode.LeftAlt] = "L-Alt",
                    [Enum.KeyCode.RightAlt] = "R-Alt",
                    [Enum.KeyCode.CapsLock] = "CAPS",
                    [Enum.KeyCode.One] = "1",
                    [Enum.KeyCode.Two] = "2",
                    [Enum.KeyCode.Three] = "3",
                    [Enum.KeyCode.Four] = "4",
                    [Enum.KeyCode.Five] = "5",
                    [Enum.KeyCode.Six] = "6",
                    [Enum.KeyCode.Seven] = "7",
                    [Enum.KeyCode.Eight] = "8",
                    [Enum.KeyCode.Nine] = "9",
                    [Enum.KeyCode.Zero] = "0",
                    [Enum.KeyCode.KeypadOne] = "Num-1",
                    [Enum.KeyCode.KeypadTwo] = "Num-2",
                    [Enum.KeyCode.KeypadThree] = "Num-3",
                    [Enum.KeyCode.KeypadFour] = "Num-4",
                    [Enum.KeyCode.KeypadFive] = "Num-5",
                    [Enum.KeyCode.KeypadSix] = "Num-6",
                    [Enum.KeyCode.KeypadSeven] = "Num-7",
                    [Enum.KeyCode.KeypadEight] = "Num-8",
                    [Enum.KeyCode.KeypadNine] = "Num-9",
                    [Enum.KeyCode.KeypadZero] = "Num-0",
                    [Enum.KeyCode.Minus] = "-",
                    [Enum.KeyCode.Equals] = "=",
                    [Enum.KeyCode.Tilde] = "~",
                    [Enum.KeyCode.LeftBracket] = "[",
                    [Enum.KeyCode.RightBracket] = "]",
                    [Enum.KeyCode.RightParenthesis] = ")",
                    [Enum.KeyCode.LeftParenthesis] = "(",
                    [Enum.KeyCode.Semicolon] = ";",
                    [Enum.KeyCode.Quote] = "'",
                    [Enum.KeyCode.BackSlash] = "\\",
                    [Enum.KeyCode.Comma] = ",",
                    [Enum.KeyCode.Period] = ".",
                    [Enum.KeyCode.Slash] = "/",
                    [Enum.KeyCode.Asterisk] = "*",
                    [Enum.KeyCode.Plus] = "+",
                    [Enum.KeyCode.Period] = ".",
                    [Enum.KeyCode.Backquote] = "`",
                    [Enum.UserInputType.MouseButton1] = "Button-1",
                    [Enum.UserInputType.MouseButton2] = "Button-2",
                    [Enum.UserInputType.MouseButton3] = "Button-3"
                }
                e.keybind.MouseButton1Click:Connect(
                    function()
                        e.keybind.Text = ". . ."
                        local g
                        g =
                            c.InputBegan:Connect(
                            function(c)
                                g:Disconnect()
                                wait()
                                local f = f[c.KeyCode] or f[c.UserInputType]
                                e.keybind.Text = f or tostring(c.KeyCode):gsub("Enum.KeyCode.", "")
                                if (c.UserInputType == Enum.UserInputType.Keyboard) then
                                    b.flags[d] = c.KeyCode
                                else
                                    b.flags[d] = c.UserInputType
                                end
                            end
                        )
                    end
                )
            end
            function i:Button(c, d)
                local d = d or function()
                    end
                local b =
                    b:Create(
                    "Frame",
                    {
                        Name = "holder",
                        Parent = h,
                        BackgroundTransparency = 1.000,
                        Position = UDim2.new(0, 10, 0, 148),
                        Size = UDim2.new(1, -20, 0, 18),
                        b:Create(
                            "TextButton",
                            {
                                BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                                Size = UDim2.new(1, 0, 1, 0),
                                BorderSizePixel = 0,
                                TextColor3 = Color3.fromRGB(255, 255, 255),
                                Text = "",
                                Name = "button",
                                AutoButtonColor = false,
                                b:Create(
                                    "TextLabel",
                                    {
                                        Name = "label",
                                        BackgroundTransparency = 1,
                                        Size = UDim2.new(1, 0, 1, 0),
                                        Font = "SourceSans",
                                        TextSize = 14,
                                        TextColor3 = Color3.fromRGB(255, 255, 255),
                                        Text = c
                                    }
                                ),
                                b:Create("UIGradient", {Rotation = 90, Color = g.gradientColorSection})
                            }
                        )
                    }
                )
                b.button.MouseButton1Click:Connect(d)
            end
            function i:Dropdown(c, d, e)
                local e = e or function()
                    end
                local f = false
                local c =
                    b:Create(
                    "Frame",
                    {
                        Name = "holder",
                        BackgroundTransparency = 1.000,
                        LayoutOrder = 3,
                        Position = UDim2.new(0, 10, 0, 98),
                        Size = UDim2.new(1, -20, 0, 36),
                        Parent = h,
                        b:Create(
                            "TextLabel",
                            {
                                Name = "label",
                                BackgroundTransparency = 1.000,
                                Size = UDim2.new(1, 0, 0, 18),
                                Font = Enum.Font.Gotham,
                                Text = c,
                                TextColor3 = Color3.fromRGB(255, 255, 255),
                                TextSize = 12.000,
                                TextXAlignment = Enum.TextXAlignment.Left
                            }
                        ),
                        b:Create(
                            "TextButton",
                            {
                                Name = "dropdown",
                                BackgroundColor3 = Color3.fromRGB(102, 41, 255),
                                BorderColor3 = Color3.fromRGB(102, 41, 255),
                                Size = UDim2.new(1, 0, 0, 18),
                                AutoButtonColor = false,
                                Font = "SourceSans",
                                Text = "",
                                TextColor3 = Color3.fromRGB(0, 0, 0),
                                TextSize = 14.000,
                                Position = UDim2.new(0, 0, 0, 18),
                                b:Create(
                                    "ImageLabel",
                                    {
                                        Name = "icon",
                                        BackgroundTransparency = 1.000,
                                        Position = UDim2.new(1, -18, 0, 0),
                                        Size = UDim2.new(0, 18, 0, 16),
                                        Image = "http://www.roblox.com/asset/?id=4641587888",
                                        ZIndex = 2
                                    }
                                ),
                                b:Create(
                                    "TextLabel",
                                    {
                                        Name = "label",
                                        BackgroundTransparency = 1.000,
                                        Position = UDim2.new(0, 4, 0, 0),
                                        Size = UDim2.new(1, -4, 0, 18),
                                        Font = Enum.Font.Gotham,
                                        Text = "Drop value",
                                        TextColor3 = Color3.fromRGB(255, 255, 255),
                                        TextSize = 12.000,
                                        TextXAlignment = Enum.TextXAlignment.Left,
                                        ZIndex = 2
                                    }
                                ),
                                b:Create(
                                    "Frame",
                                    {
                                        Name = "dropframe",
                                        BackgroundColor3 = Color3.fromRGB(32, 32, 32),
                                        BorderColor3 = Color3.fromRGB(102, 41, 255),
                                        BorderSizePixel = 0,
                                        ClipsDescendants = true,
                                        Position = UDim2.new(0, -1, 1, 1),
                                        Size = UDim2.new(1, 2, 0, 0),
                                        b:Create("UIListLayout", {SortOrder = "LayoutOrder", Padding = UDim.new(0, 2)})
                                    }
                                ),
                                b:Create(
                                    "Frame",
                                    {
                                        Name = "fill",
                                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                                        BorderSizePixel = 0,
                                        Position = UDim2.new(0, 1, 0, 1),
                                        Size = UDim2.new(1, 0, 1, 0),
                                        b:Create("UIGradient", {Rotation = 90, Color = g.gradientColorSection})
                                    }
                                )
                            }
                        )
                    }
                )
                for d, d in next, d do
                    local g =
                        b:Create(
                        "TextButton",
                        {
                            Name = "button",
                            Text = d,
                            Parent = c.dropdown.dropframe,
                            BackgroundColor3 = Color3.fromRGB(32, 32, 32),
                            BorderColor3 = Color3.fromRGB(255, 255, 255),
                            BorderSizePixel = 0,
                            Size = UDim2.new(1, 0, 0, 16),
                            Font = "SourceSans",
                            TextColor3 = Color3.fromRGB(255, 255, 255),
                            TextSize = 14.000,
                            ZIndex = 1
                        }
                    )
                    g.MouseButton1Click:Connect(
                        function()
                            f = false
                            c.dropdown.label.Text = d
                            b:Tween(c.dropdown.icon, {time = 0.1, Rotation = 180})
                            b:Tween(c, {time = 0.1, Size = UDim2.new(1, -20, 0, 35)})
                            b:Tween(c.dropdown.dropframe, {time = 0.1, Size = UDim2.new(1, 2, 0, 0)}, self.Update)
                            e(g.Text)
                        end
                    )
                end
                c.dropdown.MouseButton1Click:Connect(
                    function()
                        f = not f
                        local d = 0
                        for b, b in next, c.dropdown.dropframe:GetChildren() do
                            if (b:IsA("TextButton")) then
                                d = d + b.AbsoluteSize.Y + 2
                            end
                        end
                        if (f) then
                            b:Tween(c.dropdown.icon, {time = 0.1, Rotation = 0})
                            b:Tween(c, {time = 0.1, Size = UDim2.new(1, -20, 0, d + 50)})
                            b:Tween(c.dropdown.dropframe, {time = 0.1, Size = UDim2.new(1, 2, 0, d)}, self.Update)
                        else
                            b:Tween(c.dropdown.icon, {time = 0.1, Rotation = 180})
                            b:Tween(c, {time = 0.1, Size = UDim2.new(1, -20, 0, 36)})
                            b:Tween(c.dropdown.dropframe, {time = 0.1, Size = UDim2.new(1, 2, 0, 0)}, self.Update)
                        end
                    end
                )
            end
            function i:ColorPicker(f, i, j)
                local j = j or function()
                    end
                local i = i or Color3.fromRGB(102, 41, 255)
                local k = false
                local d = d.LocalPlayer:GetMouse()
                local f =
                    b:Create(
                    "Frame",
                    {
                        Name = "holder",
                        Parent = h,
                        BackgroundTransparency = 1.000,
                        Position = UDim2.new(0, 10, 0, 172),
                        Size = UDim2.new(1, -20, 0, 18),
                        b:Create(
                            "TextLabel",
                            {
                                Name = "label",
                                BackgroundTransparency = 1.000,
                                Size = UDim2.new(1, 0, 1, 0),
                                Font = "Gotham",
                                Text = f,
                                TextColor3 = Color3.fromRGB(255, 255, 255),
                                TextSize = 12.000,
                                TextXAlignment = Enum.TextXAlignment.Left
                            }
                        ),
                        b:Create(
                            "TextButton",
                            {
                                Name = "colorpicker",
                                BackgroundColor3 = Color3.fromRGB(34, 34, 34),
                                BorderColor3 = Color3.fromRGB(102, 41, 255),
                                Position = UDim2.new(1, -16, 0, 0),
                                Size = UDim2.new(0, 16, 0, 16),
                                AutoButtonColor = false,
                                Font = Enum.Font.SourceSans,
                                Text = "",
                                TextColor3 = Color3.fromRGB(0, 0, 0),
                                TextSize = 14.000,
                                b:Create(
                                    "Frame",
                                    {
                                        Name = "colorframe",
                                        BackgroundColor3 = Color3.fromRGB(32, 32, 32),
                                        BorderColor3 = Color3.fromRGB(102, 41, 255),
                                        Position = UDim2.new(0, -148, 1, 6),
                                        Size = UDim2.new(0, 144, 0, 139),
                                        Visible = false,
                                        ZIndex = 2,
                                        b:Create(
                                            "ImageButton",
                                            {
                                                Name = "satval",
                                                Image = "http://www.roblox.com/asset/?id=4650897272",
                                                BorderSizePixel = 0,
                                                AutoButtonColor = false,
                                                BackgroundColor3 = i,
                                                Position = UDim2.new(0, 4, 0, 3),
                                                Size = UDim2.new(0, 110, 0, 102),
                                                ZIndex = 2,
                                                b:Create(
                                                    "TextButton",
                                                    {
                                                        Name = "picker",
                                                        BackgroundColor3 = Color3.fromRGB(53, 53, 53),
                                                        BackgroundTransparency = 0.500,
                                                        BorderSizePixel = 0,
                                                        Position = UDim2.new(0.5, 0, 0.5, 0),
                                                        Size = UDim2.new(0, 5, 0, 5),
                                                        ZIndex = 2,
                                                        Text = ""
                                                    }
                                                )
                                            }
                                        ),
                                        b:Create(
                                            "TextButton",
                                            {
                                                Name = "rainbow",
                                                ZIndex = 2,
                                                BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                                                BorderColor3 = Color3.fromRGB(255, 255, 255),
                                                ClipsDescendants = true,
                                                Position = UDim2.new(0, 5, 1, -25),
                                                Size = UDim2.new(0, 133, 0, 20),
                                                AutoButtonColor = false,
                                                Font = "SourceSans",
                                                Text = "",
                                                TextColor3 = Color3.fromRGB(255, 255, 255),
                                                TextSize = 14.000,
                                                b:Create("UIGradient", {Color = g.gradientColorSection, Rotation = -90}),
                                                b:Create(
                                                    "TextLabel",
                                                    {
                                                        Name = "label",
                                                        Parent = button,
                                                        BackgroundTransparency = 1.000,
                                                        Size = UDim2.new(1, 0, 1, 0),
                                                        Font = "SourceSans",
                                                        Text = "Rainbow: OFF",
                                                        TextColor3 = Color3.fromRGB(255, 255, 255),
                                                        TextSize = 14.000,
                                                        ZIndex = 2
                                                    }
                                                )
                                            }
                                        ),
                                        b:Create(
                                            "ImageButton",
                                            {
                                                Name = "hue",
                                                AutoButtonColor = false,
                                                ZIndex = 2,
                                                Position = UDim2.new(0, 118, 0, 3),
                                                Size = UDim2.new(0, 18, 0, 102),
                                                Image = "http://www.roblox.com/asset/?id=4650897105",
                                                b:Create(
                                                    "TextButton",
                                                    {
                                                        Name = "selector",
                                                        BackgroundColor3 = Color3.fromRGB(53, 53, 53),
                                                        BorderSizePixel = 0,
                                                        Size = UDim2.new(1, 0, 0, 5),
                                                        Text = "",
                                                        ZIndex = 2
                                                    }
                                                )
                                            }
                                        )
                                    }
                                ),
                                b:Create(
                                    "Frame",
                                    {
                                        Name = "color",
                                        BackgroundColor3 = i,
                                        BorderSizePixel = 0,
                                        Position = UDim2.new(0, 1, 0, 1),
                                        Size = UDim2.new(0, 14, 0, 14)
                                    }
                                )
                            }
                        )
                    }
                )
                local g = {H = 1, S = 1, V = 1}
                local h
                local i
                local function l(b)
                    local c, d = d.X - b.AbsolutePosition.X, d.Y - b.AbsolutePosition.Y
                    local b, e = b.AbsoluteSize.X, b.AbsoluteSize.Y
                    c = math.clamp(c, 0, b)
                    d = math.clamp(d, 0, e)
                    return c / b, d / e
                end
                local function d()
                    local b = Color3.fromHSV(g.H, g.S, g.V)
                    f.colorpicker.color.BackgroundColor3 = b
                    f.colorpicker.colorframe.satval.BackgroundColor3 = Color3.fromHSV(g.H, 1, 1)
                    j(b)
                end
                f.colorpicker.MouseButton1Click:Connect(
                    function()
                        toggle = not toggle
                        f.colorpicker.colorframe.Visible = toggle
                    end
                )
                c.InputEnded:Connect(
                    function(b)
                        if b.UserInputType == Enum.UserInputType.MouseButton1 then
                            if (h) then
                                h:Disconnect()
                                h = nil
                            end
                            if (i) then
                                i:Disconnect()
                                i = nil
                            end
                            isFocused = false
                        end
                    end
                )
                f.colorpicker.colorframe.rainbow.MouseButton1Click:Connect(
                    function()
                        k = not k
                        f.colorpicker.colorframe.rainbow.label.Text = k and "Rainbow: ON" or "Rainbow: OFF"
                        if (k) then
                            repeat
                                b:Tween(
                                    f.colorpicker.colorframe.hue.selector,
                                    {time = 0.1, Position = UDim2.new(0, 0, b.rainbowI, 0)}
                                )
                                f.colorpicker.colorframe.satval.BackgroundColor3 = Color3.fromHSV(1 - b.rainbowI, 1, 1)
                                f.colorpicker.color.BackgroundColor3 = b.rainbowVal
                                j(b.rainbowVal)
                                e.Heartbeat:Wait()
                            until not k
                        end
                    end
                )
                f.colorpicker.colorframe.satval.InputBegan:Connect(
                    function(b)
                        if b.UserInputType == Enum.UserInputType.MouseButton1 then
                            if (h) then
                                h:Disconnect()
                            end
                            h =
                                e.Heartbeat:Connect(
                                function()
                                    local b, c = l(f.colorpicker.colorframe.satval)
                                    f.colorpicker.colorframe.satval.picker.Position = UDim2.new(b, 0, c, 0)
                                    g.S = b
                                    g.V = 1 - c
                                    d()
                                end
                            )
                        end
                    end
                )
                f.colorpicker.colorframe.hue.InputBegan:Connect(
                    function(b)
                        if b.UserInputType == Enum.UserInputType.MouseButton1 then
                            if (i) then
                                i:Disconnect()
                            end
                            i =
                                e.Heartbeat:Connect(
                                function()
                                    local b, b = l(f.colorpicker.colorframe.hue)
                                    g.H = 1 - b
                                    f.colorpicker.colorframe.hue.selector.Position = UDim2.new(0, 0, b, 0)
                                    d()
                                end
                            )
                        end
                    end
                )
            end
            return i
        end
    end
    return b
end
return b()
