local function b()
    local b = {flags = {}, callbacks = {}, rainbowI = 0}
    local c = game.GetService
    local d = c(game, "Players")
    local e = c(game, "RunService")
    local f = c(game, "CoreGui")
    local g = c(game, "TweenService")
    local c = c(game, "UserInputService")
    local h = d.LocalPlayer
    do
        coroutine.wrap(
            function()
                while true do
                    for c = 0, 359 do
                        b.rainbowI = c / 359
                        b.rainbowVal = Color3.fromHSV(c / 359, 1, 1)
                        wait()
                    end
                end
            end
        )()
        function b:Create(b, c)
            local b = Instance.new(b)
            for c, d in next, c do
                if c ~= "Parent" then
                    if typeof(d) == "Instance" then
                        d.Parent = b
                    else
                        b[c] = d
                    end
                end
            end
            b.Parent = c.Parent
            return b
        end
        function b:Dragger(b, c)
            local d
            local e
            local f
            local g
            local function h(b)
                local b = b.Position - f
                c:TweenPosition(
                    UDim2.new(g.X.Scale, g.X.Offset + b.X, g.Y.Scale, g.Y.Offset + b.Y),
                    "Out",
                    "Sine",
                    0.01,
                    true
                )
            end
            b.InputBegan:Connect(
                function(b)
                    if b.UserInputType == Enum.UserInputType.MouseButton1 then
                        d = true
                        f = b.Position
                        g = c.Position
                        repeat
                            wait()
                        until b.UserInputState == Enum.UserInputState.End
                        d = false
                    end
                end
            )
            b.InputChanged:Connect(
                function(b)
                    if b.UserInputType == Enum.UserInputType.MouseMovement then
                        e = b
                    end
                end
            )
            game:GetService("UserInputService").InputChanged:Connect(
                function(b)
                    if b == e and d then
                        h(b)
                    end
                end
            )
        end
        function b:Tween(b, c, d)
            local d = d or function()
                end
            local e = c.time
            c.time = nil
            local b = g:Create(b, TweenInfo.new(e), c)
            b.Completed:Connect(d)
            b:Play()
            return b
        end
        local g = {
            mainColor = Color3.fromRGB(36, 36, 36),
            bottomColor = Color3.fromRGB(34, 34, 34),
            borderColor = Color3.fromRGB(42, 42, 42),
