                for b, b in next, h:GetChildren() do
                    if (not b:IsA("UIListLayout") and not b:IsA("UIPadding") and b.Visible) then
                        c = c + UDim2.new(0, 0, 0, b.AbsoluteSize.Y + 5)
                    end
                end
                b:Tween(h, {time = 0.1, CanvasSize = c})
            end
