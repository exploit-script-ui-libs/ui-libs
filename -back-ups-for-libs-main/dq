        local i = Instance.new("TextButton", h)
        i.Modal = true
        i.BackgroundTransparency = 1
        i.Text = ""
        h.Search.textbox.Changed:Connect(
            function()
                local c = h.Search.textbox.Text:lower()
                if (c ~= "") then
                    for b, b in next, b.currentSection:GetChildren() do
                        if (not b:IsA("UIPadding") and not b:IsA("UIListLayout")) then
                            local d = b:FindFirstChild("label")
                            local e = b:FindFirstChild("button")
                            local f = false
                            if (d and d.Text:gsub("%s", ""):lower():sub(1, #c) == c) then
                                b.Visible = true
                                f = true
                            end
                            if (e and e:FindFirstChild("label") and e.label.Text:gsub("%s", ""):lower():sub(1, #c) == c) then
                                b.Visible = true
                                f = true
                            end
                            if (not f) then
                                b.Visible = false
                            end
                        end
                    end
                elseif b.currentSection then
                    for b, b in next, b.currentSection:GetChildren() do
                        if (not b:IsA("UIPadding") and not b:IsA("UIListLayout")) then
                            b.Visible = true
                        end
                    end
                end
                b.currentSectionObject:Update()
            end
        )
        b:Dragger(h.bar.bottom, h)
