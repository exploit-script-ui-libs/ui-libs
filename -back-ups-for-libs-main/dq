                                local c =
                                    math.clamp((c.X - g.slider.AbsolutePosition.X) / (g.slider.AbsoluteSize.X), 0, 1)
                                local e = f.min + (f.max - f.min) * c
                                if not f.precise then
                                    e = math.floor(e)
                                end
                                e = tonumber(string.format("%.2f", e))
                                b:Tween(g.slider.fill, {time = 0.1, Size = UDim2.new(c, 0, 1, -2)})
